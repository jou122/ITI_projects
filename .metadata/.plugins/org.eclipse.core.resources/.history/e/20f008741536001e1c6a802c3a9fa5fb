#include "../include/MRCC.h"

#define LED_PIN GPIO_PIN_13
#define LED_PORT GPIOC

void delay(volatile uint32_t count){
	while(count--);
}

int main(void){

	/*Initialize RCC */
	RCC_Init();

	/*Enable GPIO clock*/
	RCC ->AHB1ENR |= RCC_AHB1ENR_GPIOCEN;

	/*Set LED_PIN as output*/
	LED_PORT -> MODER |= GPIO_PIN_13



	return 0;
}
