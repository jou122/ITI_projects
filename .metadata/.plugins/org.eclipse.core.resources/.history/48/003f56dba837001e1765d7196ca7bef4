#ifndef MCAL_MRCC_MRCC_PRIVATE_H_
#define MCAL_MRCC_MRCC_PRIVATE_H_


#define HSEON 16
#define HSEBYP 18
#define CSSON 19
#define HSION 0

/****************RCC BASE ADDRESS*********************/
#define RCC_BASE_ADDR 0x0x40023800

typedef struct{

	u32 CR;
	u32 PLLCFGR;
	u32 CFGR;
	u32 CIR;
	u32 AHB1RSTR;
	u32 AHB2RSTR;
	u32 Reserved1;
	u32 Reserved2;
	u32 APB1RSTR;
	u32 APB2RSTR;
	u32 Reserved3;
	u32 Reserved4;
	u32 AHB1ENR;
	u32 AHB1ENR;
	u32 Reserved5;
	u32 Reserved6;

}RCC_MemMap_t;

#define RCC ((volatile RCC_MemMap_t*)(RCC_BASE_ADDR))
#endif /* MCAL_MRCC_MRCC_PRIVATE_H_ */
