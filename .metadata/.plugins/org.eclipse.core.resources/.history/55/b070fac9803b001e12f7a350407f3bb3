#ifndef MCAL_MSTK_MSTK_PRIVATE_H_
#define MCAL_MSTK_MSTK_PRIVATE_H_

#define STK_BASE_ADDR  0x40013C00


typedef struct{
	u32 CTRL;
	u32 LOAD;
	u32 VAL;
	u32 CALIB;
}MEXTI_t;



#define MEXTI ((volatile MEXTI_t *) EXTI_BASE_ADDR )



#endif /* MCAL_MSTK_MSTK_PRIVATE_H_ */
