#include"../../LTB/STD_Types.h"
#include"../../LTB/BIT_Math.h"

#include"MRCC_Int.h"
#include"MRCC_Private.h"
#include"MRCC_Config.h"

void MRCC_vInit(void){

	/*CSS (ON/OFF)*/

	/*HSEBYP CLR -> Oscillator Enable not bypassed, SET -> Oscillator Disable Bypassed*/

	CLR_BIT(RCC -> CR, HSEBYP);

	/*Select clock switch (HSI/HSE/PLL)*/
	SER_BIT(RCC -> CFGR, SW0);

	/*Enable the select clock (HSE ON/HSI ON/ PLL ON)*/
#if RCC_HSE_ENABLE == ENABLE
	SET_BIT(RCC-> CR, HSEON);
#endif
}
void MRCC_vEnableClock(/****/){



}

void MRCC_vDisableClock(/****/){


}
