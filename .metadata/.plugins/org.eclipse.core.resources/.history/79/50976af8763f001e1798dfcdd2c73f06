#include"BIT_Math.h"
#include"STD_Types.h"

#include "MGPIO_Int.h"
#include "MRCC_Int.h"
#include "MSTK_Int.h"

int main(void)
{


	MRCC_vEnableClock(RCC_AHB1,GPIOAEN);
	MRCC_vEnableClock(RCC_APB2,SYSCFGEN);


	MSTK_vInit();

	MGPIO_vSetPinMode(MGPIO_PORTA,0,MGPIO_MODE_OUTPUT);
	MGPIO_vSetPinMode(MGPIO_PORTA,1,MGPIO_MODE_OUTPUT);
	MGPIO_vSetPinMode(MGPIO_PORTA,2,MGPIO_MODE_OUTPUT);
	MGPIO_vSetPinMode(MGPIO_PORTA,3,MGPIO_MODE_OUTPUT);
	MGPIO_vSetPinMode(MGPIO_PORTA,4,MGPIO_MODE_OUTPUT);
	MGPIO_vSetPinMode(MGPIO_PORTA,5,MGPIO_MODE_OUTPUT);
	MGPIO_vSetPinMode(MGPIO_PORTA,6,MGPIO_MODE_OUTPUT);
	MGPIO_vSetPinMode(MGPIO_PORTA,7,MGPIO_MODE_OUTPUT);

  while (1)
    {

    }
  return 0;
}


void WriteToDAC(u8 Copt_SongByte)

{
	MGPIO_vSetPinVal_fast(MGPIO_PORTA,Copt_SongByte)
}

