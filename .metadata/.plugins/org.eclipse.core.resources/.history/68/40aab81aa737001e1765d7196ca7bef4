#ifndef MCAL_MRCC_MCC_PRIVATE_H_
#define MCAL_MRCC_MCC_PRIVATE_H_


/****************RCC BASE ADDRESS*********************/
#define RCC_BASE_ADDR 0x

typedef struct{

	u32 CR;
	u32 PLLCFGR;
	u32 CFGR;
	u32 CIR;
	u32 AHB1RSTR;
	u32 AHB2RSTR;
	u32 Reserved1;
	u32 Reserved2;
	u32 APB1RSTR;
	u32 APB2RSTR;
	u32 Reserved3;
	u32 Reserved4;
	u32 AHB1ENR;
	u32 AHB1ENR;
	u32 Reserved5;
	u32 Reserved6;

}RCC_MemMap_t;
#endif /* MCAL_MRCC_MCC_PRIVATE_H_ */
